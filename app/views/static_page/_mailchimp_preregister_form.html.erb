<!-- Begin MailChimp Signup Form -->
<div id="mc_embed_signup">
  <form action="//joatu.us3.list-manage.com/subscribe/post?u=6ecb9879cbdb4a555e09ca127&amp;id=8040cf5531" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
    <div id="mc_embed_signup_scroll">
      <h2><%= t('mailchimp.preregister.header_html') %></h2>
      <p><%= t('mailchimp.preregister.explanation') %></p>
      <div class="mc-field-group form-group">
        <label for="mce-EMAIL"><%= t('mailchimp.preregister.email') %> </label>
        <input type="email" value="" name="EMAIL" class="required email form-control" id="mce-EMAIL">
      </div>
      <div class="mc-field-group form-group">
        <label for="mce-POSTCODE"><%= t('mailchimp.preregister.postal') %></label>
        <input type="text" value="" name="POSTCODE" class="required form-control" id="mce-POSTCODE">
      </div>
      <div id="mce-responses" class="clear">
        <div class="response" id="mce-error-response" style="display:none"></div>
        <div class="response" id="mce-success-response" style="display:none"></div>
      </div>    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
      <div style="position: absolute; left: -5000px;"><input type="text" name="b_6ecb9879cbdb4a555e09ca127_8040cf5531" tabindex="-1" value=""></div>
      <div class="clear"><input type="submit" value="<%= t('mailchimp.preregister.signup') %>" name="subscribe" id="mc-embedded-subscribe" class="button btn btn-primary btn-lg btn-block ga-preregister-button"></div>
    </div>
  </form>
</div>
